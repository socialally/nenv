{
  "name": "nenv",
  "description": "Utility for managing node development environments",
  "version": "0.2.0",
  "author": "muji <noop@xpm.io>",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/socialally/nenv.git"
  },
  "keywords": [
    "utility",
    "environment",
    "NODE_ENV"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "~2.2.0",
    "istanbul": "~0.3.13",
    "mocha": "~2.2.4"
  },
  "engine": [
    "node >= 0.10.11"
  ],
  "scripts": {
    "docs": "npm run readme",
    "readme": "mdp --force -v",
    "test": "NODE_ENV=test mocha test/spec",
    "cover": "NODE_ENV=test istanbul cover --root lib _mocha -- test/spec"
  },
  "mdp": {
    "title": "nenv",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/socialally/nenv",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "usage.md"
        ]
      },
      {
        "inc": [
          "developer.md",
          "license.md"
        ]
      }
    ]
  }
}
